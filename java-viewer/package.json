{
  "name": "SonarJavaViewer",
  "license": "UNLICENSED",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "jquery": "3.2.1",
    "vis": "4.20.0",
    "jest": "^20.0.4"
  },
  "jest": {
    "testRegex": "src/main/resources/public/lib/viewer/__test__/.*-test.js",
    "testEnvironment": "node",
    "coverageDirectory": "target/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules"
    ]
  },
  "proxy": "http://localhost:8080"
}
